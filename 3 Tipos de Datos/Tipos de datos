package act3;


public class Act3 {

    public static void main(String[] args) {

         // Fruta:
        int precio=30;
        int numDos=5;
        int total=precio*numDos;
        System.out.println("El precio unitario es "+precio+" y si compro "+numDos+" unidades ");
        System.out.print("el total es "+total);
    }
}





